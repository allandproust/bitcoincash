        jQuery(document).ready(function(){
            
            var tmplwrap ='<p class="titulo" style="padding: 0.5rem;margin: 0;">Apostas esportivas online no Brasil</p><ul style="margin:0.2rem 0.5rem;">{contenido}</ul>';
            var tmpllink = '<li id="Element-{id}" style="border-bottom: none;{clase}"><i class="fa fa-check-circle" aria-hidden="true" style="color: #d33;font-weight: 400;font-size: 14px;top: 6px;left: 7px;"></i> <a href={link}>{texto}</a></li>';
        
            var cadena 	= '';
            var clase = '';
            var dataHtml = '';

            var element = '';
        
            dataHtml = jQuery('.widget_tablacontenido_widget').nextAll();
        
            jQuery('.space-widget-title, .space-block-title, .textwidget h2, h3',dataHtml).each(function() {

                clase = ( jQuery(this).is('h3') )  ? 'padding:0.3rem 0 0.3rem 1.5rem;' : 'padding: 0.3rem;';
        
                texto = jQuery(this).text();
        
                enlace_id = texto.replace(/\d-\s|\?|¿/g,'');
                enlace_id = enlace_id.trim();
                enlace_id = enlace_id.replace(/\s+/g, '_');

                element = jQuery(this).before('<span id="'+enlace_id+'"></span>');

                jQuery(this).attr('id',enlace_id);
                
                //Agregamos en una variable cadena
                cadena += tmpllink.replace('{link}','"#' + enlace_id + '"');
                cadena = cadena.replace('{id}', enlace_id);
                cadena  = cadena.replace('{texto}',texto);
                cadena 	= cadena.replace('{clase}',clase);
        
            });
        
            jQuery('.zone_acc_content .space-widget-title, .zone_acc_content h2').each(function() {
        
                clase = jQuery(this).hasClass('accordion-header') ? 'padding:0.3rem 0 0.3rem 1.5rem;' : 'padding: 0.3rem;';
                texto = jQuery(this).text();
        
                enlace_id = texto.replace(/\d-\s|\?|¿/g,'');
                enlace_id = enlace_id.trim();
                enlace_id = enlace_id.replace(/\s+/g, '_');
                
                jQuery(this).attr('id',enlace_id);
        
                //Agregamos en una variable cadena
                cadena += tmpllink.replace('{link}','"#' + enlace_id + '"');
                cadena = cadena.replace('{id}', enlace_id);
                cadena  = cadena.replace('{texto}',texto);
                cadena 	= cadena.replace('{clase}',clase);
        
            });
        
            cadena = tmplwrap.replace('{contenido}',cadena);
        
            jQuery('#tabla-contenido').append(cadena);
        });
        
