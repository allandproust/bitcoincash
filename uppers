<script>
			function checkMayoriaEdadBr() {
				if (localStorage.mayoriaEdadBr != 'true') {
					jQuery('body').addClass('static_age_bwd');
					jQuery('#div-cookies1').show();
				}
			}
			function mayoriaEdadBr() {
				localStorage.mayoriaEdadBr = 'true';
				jQuery('body').removeClass('static_age_bwd');
				jQuery('#div-cookies1').hide();
			}
			
			jQuery(document).ready(function() {
				checkMayoriaEdadBr();
			});
		</script>

