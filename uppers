<script>function loadCount() {
    var countDownDate = new Date('Nov 20, 2022 17:00:00 GMT+0100').getTime();
    var x = setInterval(function () {
        var now = new Date;
        var utc_timestamp = Date.UTC(now.getUTCFullYear(), now.getUTCMonth(), now.getUTCDate(),
            now.getUTCHours(), now.getUTCMinutes(), now.getUTCSeconds());
        var distance = countDownDate - utc_timestamp;
        var days = Math.floor(distance / (1000 * 60 * 60 * 24));
        var days = ("0" + days).slice(-2);
        var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
        var hours = ("0" + hours).slice(-2);
        var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
        var minutes = ("0" + minutes).slice(-2);
        var seconds = Math.floor((distance % (1000 * 60)) / 1000);
        var seconds = ("0" + seconds).slice(-2);

        let elm_countClock = document.getElementById("countClock")
        if (elm_countClock != null)
            elm_countClock.innerHTML = "<div><span class='countDay'>" + days + "</span><span class='countText'>das</span></div>:<div><span class='countHour'>" + hours + "</span><span class='countText'>horas</span></div>:<div><span class='countMinute'>" + minutes + "</span><span class='countText'>min</span></div>";
        
        if (distance < 0) {
            clearInterval(x);
            document.getElementById("countTitle").innerHTML = "Copa do Mundo Comeou";
            document.getElementById("countClock").remove();
        }
    }, 1000);
}



function showCountDown() {
    var intervalCountDown = setInterval(function () {
        if (document.querySelector('.col-3')) {
            var css = '.countdown-widget{background-image:url(https://scmedia.sportingbet.com/$-$/34678b7248fc42098ef760cc997b505c.jpg); background-size: cover; color:#000; text-align: center; padding-top: 10px;} .countDay, .countHour, .countMinute, .countSeconds  { background-color: #FFF; margin: 5px; border-radius: 3px; padding: 2px 3px;} .countText {background: none !important; color: #FFF; font-size: 0.7em; padding: 0px !important; margin: 0px !important;} p#countClock { margin-bottom: 10px; color:#fff;} p#countClock div { display: inline-grid; color:#000;} p.counttitle { color: #fff; font-weight: bold; font-size: 0.9em; margin-bottom: 5px;}',
                head = document.head || document.getElementsByTagName('head')[0],
                style = document.createElement('style');
            head.appendChild(style);
            style.type = 'text/css';
            if (style.styleSheet) {
                style.styleSheet.cssText = css;
            } else {
                style.appendChild(document.createTextNode(css));
            }
            const rightNavbar = document.getElementsByClassName("col-3");
            let div = document.createElement("div");
            div.className = "static-teaser-widget countdown-widget"
            div.innerHTML = "<a href='https://sports.sportingbet.com/pt-br/sports/futebol-4/aposta/mundo-6/copa-do-mundo-2022-0:14'><p class='counttitle'>Faltam:</p><p id='countClock'></p></a>";
            rightNavbar[0].prepend(div);

            clearInterval(intervalCountDown);
        } else if (document.querySelector('.link-list-tiles-carousel')) {
            var css = '.countdown-widget{background-image:url(https://scmedia.sportingbet.com/$-$/34678b7248fc42098ef760cc997b505c.jpg); background-size: cover; border-radius: .25rem; margin: .4375em; box-shadow: 0 1px 3px 1px rgb(0 0 0 / 20%); color:#000; text-align: center; padding-top: 10px; padding-bottom: 1px;} .countDay, .countHour, .countMinute, .countSeconds { background-color: #FFF; margin: 5px; border-radius: 3px; padding: 2px 3px;} .countText {background: none !important; color: #FFF; font-size: 0.7em; padding: 0px !important; margin: 0px !important;} p#countClock { margin-bottom: 10px; color:#fff;} p#countClock div { display: inline-grid; color:#000;} p.counttitle { color: #fff; font-weight: bold; font-size: 0.9em; margin-bottom: 5px;}',
                head = document.head || document.getElementsByTagName('head')[0],
                style = document.createElement('style');
            head.appendChild(style);
            style.type = 'text/css';
            if (style.styleSheet) {
                style.styleSheet.cssText = css;
            } else {
                style.appendChild(document.createTextNode(css));
            }
            const rightNavbar = document.getElementsByClassName("link-list-tiles-carousel");
            let div = document.createElement("div");
            div.className = "countdown-widget"
            div.innerHTML = "<a href='https://sports.sportingbet.com/pt-br/sports/futebol-4/aposta/mundo-6/copa-do-mundo-2022-0:14'><p class='counttitle'>Faltam:</p><p id='countClock'></p></a>";
            rightNavbar[0].prepend(div);

            clearInterval(intervalCountDown);
        };
    }, 700);
    loadCount();
}

setTimeout(loadCount(), 2000);
setTimeout(showCountDown(), 1000);

