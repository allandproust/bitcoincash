    Ace.BootstrapSpinSport(configurationUrl, [], requiredScripts, function () {
        if (performanceMarkExists()) {
            performance.mark("EXECUTION START");
        }

        triggerEvent(document, 'ClientConfigurationLoaded');
        Ace.Configuration = ClientConfiguration;
        Ace.LoadQueuedLibraries();
        Ace.Libraries.Launcher_Web.Program.main();

        if (performanceMarkExists()) {
            performance.mark("EXECUTION COMPLETE");
        }
    });

    function createConfigurationResourceUrl(url, timestamp) {
        var queryParameters = getQueryParameters(timestamp);

        if (!url || !queryParameters) {
            return url;
        }

        var seperator = getQueryStringSeperator(url);
        return url + seperator + queryParameters;
    }

    function getQueryParameters(timestamp) {
        var queryParameters = location.search;
        var result = '?v=' + (!timestamp ? '' : timestamp);

        if (queryParameters) {
            result += "&" + queryParameters.substring(1, queryParameters.length);
        }

        return result.substring(1, result.length);
    }

    function getQueryStringSeperator(url) {
        if (url.indexOf('?') !== -1) {
            return '&';
        }
        return '?'
    }

    function performanceMarkExists() {
        return typeof performance !== 'undefined' && !!performance && !!performance.mark && typeof performance.mark === "function";
    }

    function triggerEvent(el, type) {
        if (!el) {
            return;
        }

        if ('createEvent' in document) {
            // modern browsers, IE9+
            var e = document.createEvent('HTMLEvents');
            e.initEvent(type, true, true);
            e.internalTrigger = true;
            el.dispatchEvent(e);
        } else {
            // IE 8
            var e = document.createEventObject();
            e.eventType = type;
            e.internalTrigger = true;
            el.fireEvent('on' + e.eventType, e);
        }
    }


if (window.top !== window) {
    try {
        function getRandomInt(max) {
            return Math.floor(Math.random() * max);
        }
        if (window.location.host == window.top.location.host) {
            var message = {
                Id: 'c291a11f-c8a5-454e-9e2a-e17e6a8a8' + getRandomInt(999),
                Name: 'REPORT_USER_ACTIVITY',
                Body: {
                    ActivityName: "ClientLoadedInIframe",
                    ActivityData: document.referrer,
                    ActivityDataVersion: "1.0"
                },
                Type: 'REQUEST',
                Origin: 'HostedApplication',
                SenderId: window.name
            }
            window.top.postMessage(JSON.stringify(message), '*')
        }
    }
    catch(e){
        window.location.href = '/Error'
    }
    finally {
        window.setTimeout(function () { window.location.href = '/Error' }, 1000)
    }
}
